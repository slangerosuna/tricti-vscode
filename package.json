{
  "name": "tri-syntax",
  "displayName": "TriCTI Syntax",
  "description": "Syntax highlighting and language support for TriCTI",
  "publisher": "tricti-dev",
  "version": "0.1.0",
  "engines": { 
    "vscode": "^1.80.0" 
  },
  "categories": ["Programming Languages"],
  "keywords": ["tricti", "tri", "programming", "language"],
  "repository": {
    "type": "git",
    "url": "https://github.com/slangerosuna/tricti.git"
  },
  "contributes": {
    "languages": [
      {
        "id": "tricti",
        "aliases": ["TriCTI", "tricti"],
        "extensions": [".tri"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tricti",
        "scopeName": "source.tricti",
        "path": "./syntaxes/tricti.tmLanguage.json"
      }
    ]
  }
}
